{
  "name": "ricardom/audit-laravel",
  "description": "A simple Laravel user auditing package for Eloquent Model.",
  "keywords": [
    "ricardo"
  ],
  "homepage": "https://github.com/ricardomartos/audit-laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Ricardo Martos",
      "email": "ricardomartoslecea@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "~5.5",
    "illuminate/database": "~5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Ricardo\\Auditable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ricardo\\Auditable\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    },
    "laravel": {
      "providers": [
        "Ricardo\\Auditable\\AuditableServiceProvider"
      ]
    }
  }
}
